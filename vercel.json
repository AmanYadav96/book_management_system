{
    "builds": [{
        "src": "books_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize" : "15mb", "runtime": "python3.9.0" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "books_management_system/wsgi.py"
        }
    ]
} 